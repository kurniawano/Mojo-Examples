module bool16 (
    input a[16],
    input b[16],
    input alufn30[4],
    output out[16]
  ) {

  sig selector[16][2];
  var i;
  always {
    out = 0;
    
    for (i=0; i<16; i++){
        selector[i][1] = b[i];
        selector[i][0] = a[i]; 
    }
    
    for (i=0; i<16; i++){
        case (selector[i]){
          b00: out[i] = alufn30[0];
          b01: out[i] = alufn30[1];
          b10: out[i] = alufn30[2];
          b11: out[i] = alufn30[3];
        }
    }
  }
}
